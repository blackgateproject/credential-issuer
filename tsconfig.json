{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    // "rootDir": "./",
    "outDir": "dist",
    "strict": true,
    "sourceMap": true,
    "module": "ESNext",
    "target": "ESNext",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "esModuleInterop": true
  },
  "include": ["src/**/*.ts"]
}
